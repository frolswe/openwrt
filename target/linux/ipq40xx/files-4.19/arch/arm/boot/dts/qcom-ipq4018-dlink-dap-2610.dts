#include "qcom-ipq4019-ap.dk01.1.dtsi"
#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

/ {
	model = "D-Link DAP 2610";
	compatible = "dlink,dap-2610";

	soc {
		edma@c080000 {
			qcom,num_gmac = <1>;
		};
	};

	aliases {
		led-boot = &power;
		led-failsafe = &power;
		led-running = &power;
		led-upgrade = &power;
	};

	leds {
		compatible = "gpio-leds";

		red {
			label = "dap-2610:red:power";
			gpios = <&tlmm 4 GPIO_ACTIVE_LOW>;
		};

		power: green {
			label = "dap-2610:green:power";
			gpios = <&tlmm 5 GPIO_ACTIVE_LOW>;
		};
	};

	keys {
		compatible = "gpio-keys";

		reset {
			label = "reset";
			gpios = <&tlmm 63 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
		};
	};
};

&gmac0 {
	vlan_tag = <2 0x20>;
};

&blsp1_spi1 {
	flash@0 {
		compatible = "jedec,spi-nor";
		#address-cells = <1>;
		#size-cells = <1>;
		reg = <0>;
		spi-max-frequency = <24000000>;

		partitions {
			compatible = "fixed-partitions";

			SBL1@0 {
				label = "SBL1";
				reg = <0x0 0x40000>;
				read-only;
			};
			MIBIB@40000 {
				label = "MIBIB";
				reg = <0x40000 0x20000>;
				read-only;
			};
			QSEE@60000 {
				label = "QSEE";
				reg = <0x60000 0x60000>;
				read-only;
			};
			CDT@c0000 {
				label = "CDT";
				reg = <0xc0000 0x10000>;
				read-only;
			};
			DDRPARAMS@d0000 {
				label = "DDRPARAMS";
				reg = <0xd0000 0x10000>;
				read-only;
			};
			APPSBLENV@e0000 {
				label = "APPSBLENV";
				reg = <0xe0000 0x10000>;
				read-only;
			};
			APPSBL@f0000 {
				label = "APPSBL";
				reg = <0xf0000 0x80000>;
				read-only;
			};
			ART@170000 {
				label = "ART";
				reg = <0x170000 0x10000>;
				read-only;
			};
			firmware@180000 {
				compatible = "wrg";
				label = "firmware";
				reg = <0x180000 0xdc0000>;
			};
			rgdb@fb0000 {
				label = "rgbd";
				reg = <0xfb0000 0x10000>;
				read-only;
			};
			bdcfg@fc0000 {
				label = "bdcfg";
				reg = <0xfc0000 0x10000>;
				read-only;
			};
			langpack@fd0000 {
				label = "langpack";
				reg = <0xfd0000 0x20000>;
				read-only;
			};
			certificate@ff0000 {
				label = "certificate";
				reg = <0xff0000 0x10000>;
				read-only;
			};
			captival@f40000 {
				label = "captival";
				reg = <0xf40000 0x70000>;
				read-only;
			};
		};
	};
};

&wifi0 {
	qcom,ath10k-calibration-variant = "dlink,dap-2610";
};

&wifi1 {
	qcom,ath10k-calibration-variant = "dlink,dap-2610";
};
